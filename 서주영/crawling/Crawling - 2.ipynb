    "def checkURL(seed_urll):\n",
    "    #print(seed_urll)\n",
    "    if 'tistory' in requests.compat.urlparse(seed_urll)[1].split('.'):\n",
    "        return True\n",
    "    return False\n",
    "\n",
    "def parseURL(seed, keyword = None):\n",
    "    html = download(\"get\", seed[0])\n",
    "    dom = BeautifulSoup(html.text, \"lxml\")\n",
    "    return {(requests.compat.urljoin(seed[0],_['href']),seed[1]+1) for _ in dom.select(\"a\") if _.has_attr('href') and checkURL(requests.compat.urljoin(seed[0], _['href']))}\n",
    "        \n",
    "\n",
    "\n",
    "seed_url = \"https://search.daum.net/search\"\n",
    "param = {\"q\":\"박보영\"}\n",
    "html = download(\"get\",seed_url, param)\n",
    "dom = BeautifulSoup(html.text, \"lxml\")\n",
    "seen = list()\n",
    "queue = [(_['href'],1) for _ in dom.select(\".cont_inner>.wrap_tit>.f_link_b\") if \"tistory\" in requests.compat.urlparse(_['href'])[1].split('.') and _.has_attr('href')] # 새로운 url\n",
    "print(queue)\n",
    "\n",
    "while queue:\n",
    "    baseURL = queue.pop(0)\n",
    "    seen.append(baseURL[0])\n",
    "    print(baseURL)\n",
    "    linkList = parseURL(baseURL)\n",
    "    #print(linkList)\n",
    "    for link in linkList:\n",
    "        if link not in queue and link not in seen and link[1]<3:\n",
    "            queue.append(link)\n",
    "    print(\"Queue: {0}, Seen:{1}\".format(len(queue), len(seen)))\n"
